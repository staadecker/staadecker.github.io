{
	"name": "staadecker.github.io",
	"packageManager": "yarn@4.11.0",
	"type": "module",
	"devDependencies": {
		"@11ty/eleventy": "^3.1.2",
		"@11ty/eleventy-img": "^6.0.4",
		"@11ty/eleventy-navigation": "^1.0.4",
		"@11ty/eleventy-plugin-rss": "^2.0.4",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
		"@shikijs/markdown-it": "^3.15.0",
		"@vscode/markdown-it-katex": "^1.1.2",
		"cross-env": "^7.0.3",
		"eleventy-plugin-toc": "^1.1.5",
		"luxon": "^3.6.1",
		"markdown-it": "^14.1.0",
		"markdown-it-anchor": "^9.2.0",
		"markdown-it-footnote": "^4.0.0",
		"zod": "^3.25.67",
		"zod-validation-error": "^3.5.2"
	},
	"dependencies": {
		"@zachleat/heading-anchors": "^1.0.5"
	},
	"scripts": {
		"build": "yarn exec eleventy",
		"build-nocolor": "cross-env NODE_DISABLE_COLORS=1 yarn exec eleventy",
		"start": "yarn exec eleventy --serve --quiet",
		"debug": "cross-env DEBUG=Eleventy* yarn exec eleventy",
		"debugstart": "cross-env DEBUG=Eleventy* yarn exec eleventy --serve --quiet",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* yarn exec eleventy"
	}
}
